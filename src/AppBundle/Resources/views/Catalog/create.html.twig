{% extends 'base.html.twig' %}



{% block content%}
    <div class="container">
        <div class="row col-lg-6 col-md-offset-3">
            <form method="POST">
                <div>
                    {{ form_row(form.name) }}
                    {{ form_row(form.description) }}
                    {{ form_row(form.category) }}
                    {{ form_row(form.startDate) }}
                    {{ form_row(form.items) }}
                    {{ form_rest(form) }}
                </div>
                <div class="col-lg-6 col-md-offset-3">
                    <button class="btn btn-default" type="submit">Create catalog</button>
                </div>
            </form>
        </div>
    </div>
{% endblock %}

